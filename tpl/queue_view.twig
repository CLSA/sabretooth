 # View queue details.
 # @author Patrick Emond <emondpd@mcmaster.ca>
 # @see base_view.twig for parameters
 #}
{% extends "base_view.twig" %}

{% block pre_edit %}
  
  {% if site_id is defined and qnaire_id is defined %}
    // add the current site and qnaire to the args
    args.site_id = {{ site_id }};
    args.qnaire_id = {{ qnaire_id }};
  {% endif %}

{% endblock pre_edit %}

{% block record %}

  {{ parent() }}
  {% from 'macros.twig' import include_list %}
  {{ include_list( 'queue_state', queue_state_list|default(false), true ) }}
  {{ include_list( 'participant', participant_list|default(false), true ) }}

{% endblock record %}
